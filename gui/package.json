    "axios": "0.18.0",
    "license-checker": "^25.0.1",
    "lodash": "4.17.10",
    "material-design-icons-iconfont": "^3.0.3",
    "normalize.css": "8.0.0",
    "nprogress": "0.2.0",
    "path": "^0.12.7",
    "socket.io-client": "^2.1.1",
    "vue": "2.5.16",
    "vue-meta": "1.5.0",
    "vue-router": "3.0.1",
    "vue-socket.io": "^2.1.1-b",
    "vuetify": "^1.0.19",
    "vuex": "3.0.1"
  },
