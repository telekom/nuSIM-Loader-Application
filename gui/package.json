    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "22.4.x",
    "babel-loader": "8.0.0-beta.2",
    "cross-env": "5.1.x",
    "express": "4.16.x",
    "hygen": "1.5.x",
    "imagemin-lint-staged": "0.3.x",
    "lint-staged": "7.1.x",
    "markdownlint-cli": "0.9.x",
    "stylelint": "9.2.x",
    "stylelint-config-css-modules": "1.2.x",
    "stylelint-config-prettier": "3.2.x",
